/*
 * Broadcom BCM63148 Reference Board (REF) DTS
 */

/dts-v1/;

#include "bcm63148.dtsi"

/ {
	compatible = "brcm,BCM963148REF", "brcm,bcm63148";
	model = "Broadcom BCM963148REF";

	chosen {
		bootargs = "console=ttyS0,115200 earlyprintk";
		stdout-path = &serial0;
	};

	memory {
		reg = <0x0 0x20000000>;
	};

};

&serial0 {
	status = "okay";
};

&serial1 {
	status = "okay";
};

&nand {
	status = "okay";

	nandcs@0 {
		compatible = "brcm,nandcs";
		reg = <0>;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		brcm,nand-oob-sectors-size = <16>;
	};
};
